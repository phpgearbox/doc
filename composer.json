{
	"name": "gears/doc",
	"description": "API Documentation Generator using Markdown",
	"homepage": "https://github.com/phpgearbox/doc",
	"keywords": ["api", "documentation", "generator", "markdown"],
	"license": "MIT",
	"autoload":
	{
		"psr-4":
		{
			"Gears\\Doc\\": "src"
		}
	},
	"require":
	{
		"gears/string": "*",
		"gears/arrays": "*",
		"gears/view": "*",
		"erusev/parsedown": "1.*",
		"symfony/console": "2.*",
		"symfony/finder": "2.*",
		"symfony/filesystem": "2.*"
	},
	"require-dev":
	{
		"phpunit/phpunit": "4.*",
		"codegyre/robo": "*",
		"guzzlehttp/guzzle": "4.*"
	},
	"bin": ["gearsdoc"]
}